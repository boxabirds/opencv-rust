{
  "name": "opencv-rust-web-benchmark",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:check && vite",
    "dev:check": "node scripts/check-wasm.js || npm run build:wasm",
    "build:wasm": "cd ../../ && rm -rf pkg && wasm-pack build --target web --out-dir pkg --features gpu,wasm",
    "build:wasm:force": "npm run build:wasm",
    "build": "npm run build:wasm && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.553.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "puppeteer": "^24.30.0",
    "vite": "^5.0.0",
    "vite-plugin-wasm": "^3.3.0"
  }
}
